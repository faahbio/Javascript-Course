{
  "manifest_version": 3,
  "name": "4Later",
  "version": "0.2",
  "description": "A simple extension to save links 4 later",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon48.png"
  },
  "permissions": ["tabs"],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
